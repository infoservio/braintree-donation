<html xmlns="http://www.w3.org/1999/xhtml" lang="en-US">
<head>
    <meta name="viewport" content="width=device-width">
    {% block head %}{% endblock %}
</head>
<body>
<div class="overlay">
</div>
    <div class="success-panel">
        <img class="success-logo" />
        <div class="panel-body">
            <div style="display: flex; justify-content: space-between;">
                <div>
                <a href="{{ baseUrl }}">Return back to the page</a>
                </div>
                <div>
                    <h4><span>$ </span><span  id="success-amount">10000</span></h4>
                </div>
            </div>
            <div class="successMessage">
                {{ successMessage | raw }}
            </div>
            <div>
                <h2>Project Information</h2>
            </div>
            <div class="container">
                <div class="row">
                    <div class="col-sm-6">
                        <label>Project Name</label>
                        <p>{{ resetForm['projectName'] }}</p>
                    </div>
                    <div class="col-sm-6">
                        <label for="test">Project ID</label>
                        <p for="test">{{ resetForm['projectId'] }}</p>
                    </div>
                </div>
            </div>
            <div>
                <h2>Contact Information</h2>
            </div>
            <div class="container">
                <div class="row">
                    <div class="col-sm-6">
                        <label>First Name</label>
                        <p>{{ resetForm['firstName'] ? resetForm['firstName'] : '-' }}</p>
                    </div>
                    <div class="col-sm-6">
                        <label for="test">Last Name</label>
                        <p for="test">{{ resetForm['lastName'] ? resetForm['lastName'] : '-' }}</p>
                    </div>
                    <div class="col-sm-6">
                        <label>Email</label>
                        <p>{{ resetForm['email'] ? resetForm['email'] : '-' }}</p>
                    </div>
                    <div class="col-sm-6">
                        <label for="test">Phone</label>
                        <p for="test">{{ resetForm['phone'] ? resetForm['phone'] : '-' }}</p>
                    </div>
                </div>
            </div>
            <div>
                <h2>Address Information</h2>
            </div>
            <div class="container">
                <div class="row">
                    <div class="col-sm-6">
                        <label for="test">Postal Code</label>
                        <p for="test">{{ resetForm['postalCode'] ? resetForm['postalCode'] : '-' }}</p>
                    </div>
                    <div class="col-sm-6">
                        <label for="test">Country</label>
                        <p for="test">{{ resetForm['countryId'] ? resetForm['countryId'] : '-' }}</p>
                    </div>
                    <div class="col-sm-6">
                        <label>State</label>
                        <p>{{ resetForm['state']  ? resetForm['state'] : '-' }}</p>
                    </div>
                    <div class="col-sm-6">
                        <label for="test">City</label>
                        <p for="test">{{ resetForm['city'] ? resetForm['city'] : '-' }}</p>
                    </div>
                    <div class="col-sm-6">
                        <label>Address 1</label>
                        <p>{{ resetForm['streetAddress'] ? resetForm['streetAddress'] : '-' }}</p>
                    </div>
                    <div class="col-sm-6">
                        <label for="test">Address 2</label>
                        <p for="test">{{ resetForm['extendedAddress'] ? resetForm['extendedAddress'] : '-' }}</p>
                    </div>
                    <div class="col-sm-6">
                        <label>Company</label>
                        <p>{{ resetForm['company'] ? resetForm['company'] : '-' }}</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>