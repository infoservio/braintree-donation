{
    "name": "endurant/donations-free",
    "description": "Free Braintree Donation System",
    "type": "craft-plugin",
    "version": "1.0.10",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "donations-free"
    ],
    "support": {
        "docs": "http://test",
        "issues": "http://test"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "endurant",
            "homepage": "https://endurant.org"
        }
    ],
    "require": {
        "craftcms/cms": "~3.0.0-beta.23",
        "braintree/braintree_php" : "3.6.1",
        "yiisoft/yii2": "^2.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
          "endurant\\donationsfree\\": "src/"
        }
    },
    "extra": {
        "name": "Donations Free",
        "handle": "donations-free",
        "schemaVersion": "1.0.0",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "???",
        "components": {
            "donationService": "endurant\\donationsfree\\services\\DonationService",
            "braintreeService": "endurant\\donationsfree\\services\\BraintreeService",
            "cardService": "endurant\\donationsfree\\services\\CardService",
            "customerService": "endurant\\donationsfree\\services\\CustomerService",
            "addressService": "endurant\\donationsfree\\services\\AddressService",
            "transactionService": "endurant\\donationsfree\\services\\TransactionService",
            "logService": "endurant\\donationsfree\\services\\LogService",
            "braintreeHttpClient": "endurant\\donationsfree\\components\\httpClient\braintree\\BraintreeHttpClient",
            "donationLogger": "endurant\\donationsfree\\components\\logger\\Logger",
            "csvParser": "endurant\\donationsfree\\components\\parser\\CsvParser"
        },
        "class": "endurant\\donationsfree\\DonationsFree"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    }
}
